<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.kwameasante.bucketlist.MainActivity"
    tools:showIn="@layout/activity_main"
    android:clickable="false">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="See a Movie at the Virginia Film Festival"
        android:id="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Pick Fruit at Carter Mountain"
        android:id="@+id/textView3"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Streak the Lawn"
        android:id="@+id/textView4"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Remove yourself from facebook for 24 hours"
        android:id="@+id/textView5"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="View U.Va. from the roof of a building"
        android:id="@+id/textView6"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Look for ghosts in the U. Va. cemetery"
        android:id="@+id/textView7"
        android:layout_marginTop="48dp"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Read a book in your favorite garden"
        android:id="@+id/textView8"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="63dp"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Run or support a Charlottesville road race"
        android:id="@+id/textView9"
        android:layout_marginTop="56dp"
        android:layout_below="@+id/textView8"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="See a horse at Foxfield"
        android:id="@+id/textView10"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="strike"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Go to an international party"
        android:id="@+id/textView11"
        android:layout_marginTop="59dp"
        android:layout_below="@+id/textView10"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="strike"/>

</RelativeLayout>
